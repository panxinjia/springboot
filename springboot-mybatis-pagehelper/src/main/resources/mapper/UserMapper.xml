<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xiaopantx.springbootmybatispagehelper.mapper.UserMapper">

    <select id="getUserById" resultType="com.xiaopantx.springbootmybatispagehelper.entity.User">
        select * from t_user where id = #{id}
    </select>
    <select id="selectList" resultType="com.xiaopantx.springbootmybatispagehelper.entity.User">
        select * from t_user
    </select>
</mapper>